{
    "name": "@aurora-launcher/core",
    "version": "0.24.0",
    "description": "Base library for Aurora Launcher",
    "type": "commonjs",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/types/index.d.ts",
    "exports": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.js"
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "prettier": "prettier --write src",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AuroraTeam/AuroraLauncher.git"
    },
    "author": "AuroraTeam",
    "contributors": [
        "JoCat (https://github.com/JoCat)"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/AuroraTeam/AuroraLauncher/issues"
    },
    "homepage": "https://github.com/AuroraTeam/AuroraLauncher#readme",
    "importSort": {
        ".js, .ts": {
            "style": "module",
            "parser": "typescript"
        }
    },
    "files": [
        "dist",
        "src"
    ],
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@tsconfig/node20": "^20.1.4",
        "@tsconfig/strictest": "^2.0.5",
        "@types/node": "^22.9.0",
        "eslint": "^9.14.0",
        "globals": "^15.12.0",
        "import-sort-style-module": "^6.0.0",
        "prettier": "^3.3.3",
        "prettier-plugin-import-sort": "0.0.7",
        "tsup": "^8.3.5",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.13.0"
    },
    "dependencies": {
        "adm-zip": "^0.5.16",
        "p-map": "^7.0.2",
        "undici": "^6.20.1"
    }
}
